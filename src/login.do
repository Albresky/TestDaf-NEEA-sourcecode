<script language="javascript" type="text/javascript">
	$(function() {
		if (window.location.protocol !== 'https:') {
			window.location.href = getSslHost() + window.location.pathname + window.location.search;
			return;
		}
		getCheckImage();
		$("#btnLogin").click(function() {
			$("#serverError").text("");
			if ($("#userId").val() == "") {
				$("#msg").text("请输入登录的证件号码");
				return false;
			}
			if ($("#pwd").val() == "") {
				$("#msg").text("请输入登录密码");
				return false;
			}
			if ($("#checkImgCode").val() == "") {
				$("#msg").text("请输入验证码");
				return false;
			}

			var token = $("#userId").val().toUpperCase() + $.trim($("#pwd").val());
			$("#token").val($.md5(token));
			$("#loginInfo").submit();
			return true;
		});
		$("#checkImgCode").val('');
		gotoNews();
	});
	function goResetPwd(){
		window.location.href = "/resetpwdstep1.do";
	}
